<h3>Shorted URLs</h3>
<div id="short-urls_page">
    <table>
        <tr>
            <th>Short URL</th>
            <th>Original URL</th>
        </tr>
        {{#each data}}
            <tr>
                <td class="center">{{ this.slno }}</td>
                <td><a href="http://{{ this.shortURL }}">{{ this.shortURL }}</a></td>
                <td><a href="http://{{ this.longURL }}">{{ this.longURL }}</a></td>
            </tr>
        {{/each}}
    </table>

    {{!-- FLOATING MENU  --}}
    {{>floatingMenu}}

</div>